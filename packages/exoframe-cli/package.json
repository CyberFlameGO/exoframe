{
  "name": "exoframe",
  "version": "7.0.0",
  "description": "Exoframe is a self-hosted tool that allows simple one-command deployments using Docker",
  "main": "dist/index.js",
  "type": "module",
  "repository": "git@github.com:exoframejs/exoframe.git",
  "author": "Tim Ermilov <yamalight@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir lib/ --watch",
    "lint": "eslint src/",
    "test": "TZ=Greenland NODE_ENV=testing NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:ci": "TZ=Greenland NODE_ENV=testing NODE_OPTIONS=--experimental-vm-modules jest --collectCoverage"
  },
  "dependencies": {
    "apache-md5": "^1.1.7",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "exoframe-client": "^7.0.0",
    "got": "^11.8.2",
    "htm": "^3.1.0",
    "ink": "^3.2.0",
    "ink-select-input": "^4.2.0",
    "ink-select-input-horizontal": "^4.5.1",
    "ink-text-input": "^4.0.1",
    "lodash": "^4.17.21",
    "open": "^8.4.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "ink-testing-library": "^2.1.0",
    "jest": "^27.3.1",
    "nock": "^13.1.4",
    "prettier": "^2.4.1",
    "tar-fs": "^2.1.1"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "setupFiles": [
      "<rootDir>/.jest/setEnv.js"
    ]
  },
  "engines": {
    "node": ">=16"
  }
}
